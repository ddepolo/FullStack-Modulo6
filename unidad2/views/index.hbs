        <td><a href="/borrar:id={{id}}"><img src="images/x-circle-fill.svg"> </a></td>
        <td>{{id}}</td>
        <td>{{nombre}}</td>
        <td>{{tel}}</td>
        <td>{{dir}}</td>
      </tr>
    {{/each}}

</table>

</div>

