<div class="container-fluid">
  <div class="row titulos">
     <div class="col">
        <h2>Empleados</h2>
        <p>Listado y modificación</b></p>
     </div>
  </div>

  <table class="table">
    <tr>
      <tr colspan="2"></tr>
      <tr>ID</tr>
      <tr>Nombre</tr>
      <tr>Teléfono</tr>
      <tr>Dirección</tr>
    </tr>
    {{#each resu}}
      <tr>
